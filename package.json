{
  "name": "workergen-backend",
  "version": "3.0.0",
  "description": "MajorGen: Harness AI to craft tailored, ATS-optimized resumes and cover letters quickly and efficiently.",
  "main": "src/index.ts",
  "engines": {
    "node": ">=20.5.1"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "build": "tsc --build",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js **/*.ts",
    "prettier:fix": "prettier --write **/*.js **/*.ts",
    "docker:prod:build": "docker compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "docker:dev:build": "docker compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "docker:test:build": "docker compose -f docker-compose.yml -f docker-compose.test.yml build",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:test": "docker compose -f docker-compose.yml -f docker-compose.test.yml up --build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@aws-sdk/lib-storage": "^3.565.0",
    "@aws-sdk/s3-request-presigner": "^3.565.0",
    "@sentry/cli": "^2.31.2",
    "@sentry/node": "^7.113.0",
    "@sentry/profiling-node": "^7.113.0",
    "aws-sdk": "^2.1610.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^10.0.0",
    "exa-js": "^1.3.3",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.7.2",
    "moment": "^2.24.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.3.1",
    "openai": "^4.77.0",
    "openapi-types": "^12.1.3",
    "papaparse": "^5.5.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pdf-parse": "^1.1.1",
    "pm2": "^5.1.0",
    "redis": "^4.7.0",
    "semver": "^7.5.2",
    "stripe": "^15.7.0",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "tesseract.js": "^5.0.5",
    "tough-cookie": "^4.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "uuid": "^11.1.0",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.12.8",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/papaparse": "^5.3.16",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/stripe": "^8.0.417",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.11.9",
    "coveralls": "^3.0.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-security": "^1.7.1",
    "faker": "^5.1.0",
    "husky": "^7.0.4",
    "jest": "^26.0.1",
    "lint-staged": "^11.2.6",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.8.8",
    "supertest": "^6.0.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown ."
  }
}
